    <div class="modal fade" id="staticModal1" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
      <div class="modal-dialog modal-fullscreen" style="width: 100%;">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="staticBackdropLabel">Modal title</h5>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
          </div>
          <div class="modal-body">
            <a class="btn btn-secondary" href="http://localhost:3000/home">MAIN</a>
       
    


                <% for(let char of character) { %>

              
                
     <%- include('created.ejs', {charFullname : char.fullname , Race: char.RaceAlign,HP: char.HitPoints, AC: char.ArmorClass}) %>
   
                  <% }%>
             
           
      </div>
    </div>
    </div>
    </div>










