    <script>
       const box1 = document.getElementById('staticBackdrop1');
       const box2 = document.getElementById('staticBackdrop2');
       const box3 = document.getElementById('staticBackdrop3');
       const box4 = document.getElementById('staticBackdrop4');
       var myModal1 = new bootstrap.Modal(document.getElementById('staticModal1'), {
