}
 .content {
	 margin: auto auto;
	 position: relative;
	 width: 200px;
	 height: 200px;
	 perspective: 1500px;
}
 .die {
	 position: absolute;
	 width: 100%;
	 height: 100%;
	 transform-style: preserve-3d;
	 transition: transform 0.5s ease-out;
	 cursor: pointer;
	 transform: rotateX(-53deg);
}
 .die.rolling {
	 animation: roll 3s linear;
}
 .die[data-face="1"] {
	 transform: rotateX(-53deg) rotateY(0deg);
   
}
